{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist",
  "functions": {
    "dist/api/index.js": {
      "runtime": "nodejs@20.x",
      "memory": 256
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
